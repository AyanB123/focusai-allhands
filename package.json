{
  "name": "productivitypro",
  "version": "0.1.0",
  "description": "AI-powered productivity tracking and optimization application",
  "main": ".webpack/main/main.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "test": "jest",
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --watch --config webpack.config.js",
    "ml:install": "cd ml && pip install -r requirements.txt",
    "ml:start": "cd ml && python server.py",
    "ml:load-sample": "cd ml && python data/load_sample_data.py",
    "ml:test": "cd ml && python test_ml_components.py",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "productivity",
    "time-tracking",
    "ai",
    "machine-learning",
    "electron"
  ],
  "author": "ProductivityPro Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AyanB123/focusai-allhands.git"
  },
